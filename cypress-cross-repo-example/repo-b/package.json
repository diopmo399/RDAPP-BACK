{
  "name": "cypress-e2e-tests",
  "version": "1.0.0",
  "description": "Cypress E2E tests for cross-repo testing",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:headed": "cypress run --headed",
    "cy:run:spec": "cypress run --spec",
    "test": "cypress run",
    "test:staging": "cypress run --config baseUrl=https://staging.example.com",
    "test:preprod": "cypress run --config baseUrl=https://preprod.example.com"
  },
  "keywords": [
    "cypress",
    "e2e",
    "testing",
    "github-actions"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.6.2",
    "cypress-multi-reporters": "^1.6.4",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {}
}
